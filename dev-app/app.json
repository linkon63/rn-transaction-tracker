{
  "expo": {
    "name": "MM Tracker",
    "slug": "mm-tracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "owner": "linkonexpo63",
    "icon": "./assets/images/icon.png",
    "scheme": "rncrud",
    "userInterfaceStyle": "automatic",
    "runtimeVersion": "1.0.0",
    "platforms": ["ios", "android", "web"],
    "newArchEnabled": false,
    "ios": {
      "supportsTablet": true
    },
    "updates": {
      "enabled": true,
      "url": "https://u.expo.dev/b5c14ed3-6405-40f0-a72e-974a885d6f7e"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.linkon63.mmtracker"
    },
    "web": {
      "bundler": "metro",
      "output": "server",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "acbd3ce2-764e-40ff-ad15-3ceef687fd03"
      }
    }
  }
}
